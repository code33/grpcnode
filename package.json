{
  "name": "grpcnode",
  "version": "1.0.0",
  "description": "CLI tool for working with gRPC in nodejs, dynamically",
  "scripts": {
    "build": "babel src -d build",
    "prepublish": "npm run build"
  },
  "bin": {
    "grpcnode": "build/grpcnode.js"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konsumer/grpcnode.git"
  },
  "author": "David Konsumer <konsumer@jetboystudio.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/konsumer/grpcnode/issues"
  },
  "homepage": "https://github.com/konsumer/grpcnode#readme",
  "keywords": [
    "grpc",
    "api",
    "protobuf"
  ],
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "plugins": [
      "shebang"
    ]
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "grpc": "^1.10.1",
    "indent": "0.0.2",
    "json-colorizer": "^1.1.0",
    "lodash": "^4.17.5",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-shebang": "0.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1"
  }
}
